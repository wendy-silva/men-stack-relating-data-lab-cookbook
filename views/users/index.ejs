<!-- views/users/index.ejs -->
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Community</title>
</head>
<body>
    <%- include('../partials/_navbar.ejs') %>
    <ul>
        <% if (users.length > 0) { %>
            <% users.forEach(user => { %>
                <li><a href="/users/<%= user._id %>"><%= user.username %></a></li>
            <% }) %>
        <% } else { %>
            <li>No users found</li>
        <% } %>
    </ul>
</body>
</html>